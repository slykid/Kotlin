<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.slyki.myfirstkotlin">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".BmiJavaActivity"
                android:label="BmiJavaActivity"/>
        <activity
                android:name=".BmiKotlinActivity"
                android:label="BmiKotlinActivity"/>
   <!--------------------------------- 추가부분 -------------------------------------->
        <activity android:name=".variableJavaActivity" android:label="variableJava">
        </activity>
        <activity android:name=".variableKotlinActivity" android:label="variableKotlin">
        </activity>
    </application>

</manifest>
